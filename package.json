{
  "name": "seb-mobile-tyres",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf out dist ./next ./public/signs/nextImageExportOptimizer/ ./public/signs/next-image-export-optimizer-hashes.json",
    "dev": "rm -rf out dist ./next && next dev",
    "build": "next build",
    "export": "next export && next-image-export-optimizer",
    "start": "next start",
    "lint": "next lint",
    "serve": "npm run clean && npm run build && npm run export && npx serve out",
    "postbuild": "next-sitemap"
  },
  "dependencies": {
    "@next/font": "13.1.0",
    "@types/node": "18.11.17",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "next": "13.1.0",
    "next-image-export-optimizer": "^1.1.2",
    "next-pwa": "^5.6.0",
    "next-sitemap": "^3.1.43",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.57.1",
    "typescript": "4.9.4"
  }
}
